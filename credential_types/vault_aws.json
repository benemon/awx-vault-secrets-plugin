{
  "name": "Vault AWS Dynamic Credential",
  "kind": "cloud",
  "inputs": {
    "fields": [
      {
        "id": "url",
        "type": "string"
      },
      {
        "id": "auth_method",
        "type": "string",
        "choices": [
          "token",
          "approle",
          "jwt"
        ]
      },
      {
        "id": "token_value",
        "type": "string"
      },
      {
        "id": "approle_role_id",
        "type": "string"
      },
      {
        "id": "approle_secret_id",
        "type": "string"
      },
      {
        "id": "jwt_role",
        "type": "string"
      },
      {
        "id": "jwt_token",
        "type": "string"
      },
      {
        "id": "namespace",
        "type": "string"
      },
      {
        "id": "engine_type",
        "type": "string",
        "default": "dynamic"
      },
      {
        "id": "dynamic_engine",
        "type": "string",
        "default": "aws"
      },
      {
        "id": "mount",
        "type": "string"
      },
      {
        "id": "path",
        "type": "string"
      }
    ]
  },
  "injectors": {
    "env": {
      "AWS_ACCESS_KEY_ID": "{{ aws_access_key }}",
      "AWS_SECRET_ACCESS_KEY": "{{ aws_secret_key }}",
      "AWS_SESSION_TOKEN": "{{ aws_session_token }}"
    }
  }
}